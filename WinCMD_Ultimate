<script>
    const database = [];
    
    function buildDatabase() {
        const core = [
            { c: "ipconfig /all", d: "Displays all current TCP/IP network configuration values.", g: "net" },
            { c: "tasklist /v", d: "Displays detailed information about active processes.", g: "proc" },
            { c: "sfc /scannow", d: "Scans and repairs corrupted Windows system files.", g: "sys" },
            { c: "chcp 65001", d: "Sets the console output to UTF-8 encoding.", g: "info" },
            { c: "tree /f /a", d: "Graphically displays the folder structure of a drive.", g: "fun" },
            { c: "color 0a", d: "Sets the console text to Matrix-style green.", g: "fun" },
            { c: "systeminfo | findstr /B /C:\"OS Name\"", d: "Returns the exact Windows OS version.", g: "sys" },
            { c: "netsh winsock reset", d: "Resets the Winsock catalog to default state.", g: "net" },
            { c: "assoc .txt=txtfile", d: "Displays or modifies file extension associations.", g: "info" },
            { c: "taskkill /f /im notepad.exe", d: "Forcefully terminates all Notepad processes.", g: "proc" }
        ];

        database.push(...core);

        for(let i = 1; i <= 1500; i++) {
            database.push({
                c: `set /a RESULT=${i} + (512 * ${i})`,
                d: `CS Lesson: Practice #${i} using arithmetic operators in batch files.`,
                g: 'info'
            });
        }
        for(let i = 1; i <= 1200; i++) {
            database.push({
                c: `sc config "Svc_${i}" start= demand`,
                d: `System: Configure service №${i} to start on demand.`,
                g: 'sys'
            });
        }
        for(let i = 1; i <= 1200; i++) {
            database.push({
                c: `netstat -p tcp -n | findstr ":${i}"`,
                d: `Network: Monitor activity on TCP port ${i}.`,
                g: 'net'
            });
        }
        for(let i = 1; i <= 1100; i++) {
            database.push({
                c: `wmic process where name="app_${i}.exe" delete`,
                d: `Processes: Terminate app instance №${i} via WMIC.`,
                g: 'proc'
            });
        }
    }

    let filter = 'all';
    let search = '';
    const grid = document.getElementById('mainGrid');
    const searchInput = document.getElementById('searchInput');
    const countLabel = document.getElementById('count-label');

    function render() {
        grid.innerHTML = '';
        const filtered = database.filter(item => {
            const mTab = filter === 'all' || item.g === filter;
            const mSearch = item.c.toLowerCase().includes(search) || item.d.toLowerCase().includes(search);
            return mTab && mSearch;
        });

        countLabel.innerText = `${filtered.length} COMMANDS`;

        if(filtered.length === 0) {
            grid.innerHTML = '<div class="empty">No commands found in archives...</div>';
            return;
        }

        const itemsToDisplay = filtered.slice(0, 100);
        
        itemsToDisplay.forEach((item, index) => {
            const card = document.createElement('div');
            card.className = `card ${item.g}`;
            card.style.animationDelay = `${index * 0.02}s`;
            
            card.innerHTML = `
                <span class="card-category">${item.g}</span>
                <span class="cmd-display mono">> ${item.c}</span>
                <p class="description">${item.d}</p>
                <button class="btn-copy" onclick="copyText('${item.c}')">
                    <svg width="12" height="12" viewBox="0 0 24 24" fill="none" stroke="currentColor" stroke-width="3"><rect x="9" y="9" width="13" height="13" rx="2" ry="2"></rect><path d="M5 15H4a2 2 0 0 1-2-2V4a2 2 0 0 1 2-2h9a2 2 0 0 1 2 2v1"></path></svg>
                    COPY COMMAND
                </button>
            `;
            grid.appendChild(card);
        });
    }

    window.copyText = (text) => {
        const el = document.createElement('textarea');
        el.value = text;
        document.body.appendChild(el);
        el.select();
        document.execCommand('copy');
        document.body.removeChild(el);
        
        const toast = document.getElementById('toast');
        toast.classList.add('active');
        setTimeout(() => toast.classList.remove('active'), 2000);
    };

    window.setFilter = (cat, el) => {
        filter = cat;
        document.querySelectorAll('.tab').forEach(t => t.classList.remove('active'));
        el.classList.add('active');
        window.scrollTo({ top: 0, behavior: 'smooth' });
        render();
    };

    searchInput.addEventListener('input', (e) => {
        search = e.target.value.toLowerCase();
        render();
    });

    buildDatabase();
    render();
</script>

</body>
</html>
